URxvt.perl-ext-common:  default,matcher,tabbed
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1
URxvt.tabbed.tabbar-fg: 7
URxvt.tabbed.tabbar-bg: 8	
URxvt.tabbed.tab-fg: 4
URxvt.tabbed.tab-bg: 0
URxvt.tabbed.autohide: yes
URxvt.tabbed.title: yes

Xft.autohint: 0
Xft.antialias: 1
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.dpi: 96
Xft.rgba: rgb
Xft.lcdfilter: lcddefault
URxvt*scrollTtyOutput: false

URxvt.font: xft:Ohsnap:pixelsize=8:antialias=1:hinting=hintslight,\
			-*-unifont-*-*-*-*-*-*-*-*-*-*-*-*
URxvt.boldFont: xft:Ohsnap:size=8:antialias=1:hinting=hintslight,\
			-*-unifont-*-*-*-*-*-*-*-*-*-*-*-*
URxvt*highlightColor: #D3D7CF
URxvt.scrollBar: false
URxvt*saveLines: 12000
URxvt.cursorBlink: true
URxvt.cursorUnderline: false>
URxvt.letterSpace: 0
URxvt.lineSpace: 0


URxvt*urlLauncher: firefox
URxvt*matcher.button: 1

URxvt*internalBorder: 3
URxvt*externalBorder:     0
URxvt*borderLess:         false
URxvt*borderColor: #202020

! █▓▒░ blaque sourcery
! █▓▒░ git.io/.files

! special
*.foreground:   #aaaaaa
*.background:   #222222
*.cursorColor:  #aaaaaa

! black
*.color0:       #1b1b1b
*.color8:       #202020

! red
*.color1:       #5f8787
*.color9:       #5f8787

! green
*.color2:       #6c6c6c
*.color10:      #6c6c6c

! yellow
*.color3:       #87875f
*.color11:      #87875f

! blue
*.color4:       #888888
*.color12:      #888888

! magenta
*.color5:       #5c9797
*.color13:      #5c9797

! cyan
*.color6:       #777777
*.color14:      #777777

! white
*.color7:       #c1c1c1
*.color15:      #c1c1c1
! others
URxvt.colorIT:  #c1c1c1
URxvt.colorBD:  #d3d3d3
URxvt.colorUL:  #c1c1c1

!URxvt.keysym.C-Up: command:\033]720;1\007
!URxvt.keysym.C-Down: command:\033]721;1\007


!URxvt.keysym.Up : 33[A !breaks arrow keys in Vim
!URxvt.keysym.Down : 33[B !breaks arrow keys in Vim
!URxvt.keysym.Right : 33[C !breaks arrow keys in Vim
!URxvt.keysym.Left : 33[D !breaks arrow keys in Vim
!URxvt.keysym.Page_Up : 33[5~ !breaks scrolling in Rxvt
!URxvt.keysym.Page_Down : 33[6~ !breaks scrolling in Rxvt
!URxvt.keysym.Home : 33[H !breaks navigation in Vim
!URxvt.keysym.End : 33[F !breaks navigation in Vim
URxvt.keysym.S-Up : 33[1;2A
URxvt.keysym.S-Down : 33[1;2B
URxvt.keysym.S-Right : 33[1;2C
URxvt.keysym.S-Left : 33[1;2D

